{
  "name": "ladderly-io",
  "version": "0.1.2",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "npm run lint && next build",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev",
    "install:vercel": "npm install && npm rebuild argon2 --build-from-source && npm run postinstall",
    "vercel:build": "next build",
    "vercel:local-build": "npm run install:vercel && npm run vercel:build",
    "vercel:deploy-preview-from-local": "vercel deploy --cwd .. --yes --debug",
    "vercel:preview": "npm run vercel:local-build && npm run vercel:deploy-preview-from-local",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start"
